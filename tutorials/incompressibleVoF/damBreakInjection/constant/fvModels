FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvModels;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

turbulenceDamping
{
    type            VoFTurbulenceDamping;

    delta           1e-4;
}

injection
{
    type            volumeSource;

    phase           water;

    points          ((0.438 0.438 0.0073));

    volumetricFlowRate 0.0003;
}


